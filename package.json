{
  "name": "GreenDocs",
  "version": "1.0.0",
  "description": "Greenstand documentation",
  "main": "server.js",
  "author": "Phil Norcross (philn@philnorcross.com)",
  "license": "ISC",
  "nodemonConfig": {
    "ignoreProduction": [
      "./*"
    ],
    "watch": [
      "*.js",
      "pnconfig.json"
    ],
    "ignore": [
      "a.*",
      "*_*",
      "*.sh",
      "logs.d/*",
      "kill",
      "test.js",
      "start",
      "stop",
      "d0",
      "greenorg/*"
    ]
  },
  "scripts": {
    "start": "nodemon -V -e js,json,txt,htm --watch ./",
    "test": "test.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  },
  "engines": {"node": ">=8.11.1"},
  "xrepository": {
    "type": "git",
    "url": "git+https://github.com/philnorc/greenstand-documentation.git"
  },
  "keywords": ["Greenstand","documentation"],
  "bugs": {"url": "https://github.com/philnorc/greenstand-documentation/issues"},
  "homepage": "https://github.com/philnorc/greenstand-documentation#readme"
}
